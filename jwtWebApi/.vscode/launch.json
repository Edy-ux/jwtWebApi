    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core JWTAPI (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/bin/Debug/net9.0/JwtWebApi.dll",
            "args": [],
            "debugServer": "${workspaceFolder}/bin/Debug/net9.0/JwtWebApi.dll",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_HOSTINGSTARTUPAPPLICATION": "app.dll"
            },
            "nativeDebugging": false,
            "console": "integratedTerminal"
        }
    ]
}
